@{
    ViewBag.Title = "SiteDeepDive";
    Layout = "~/Views/Shared/_ECommerce.cshtml";
}

@*@Styles.Render("~/Content/Tables/Tables.css?" + @iSHOPNew.DAL.GlobalVariables.GetRandomNumber + "")
@Scripts.Render("~/Scripts/E_Commerce_Table/SiteDeepDive.js?" + @iSHOPNew.DAL.GlobalVariables.GetRandomNumber + "")
@Scripts.Render("~/Scripts/E_Commerce_Table/Table-Common.js?" + @iSHOPNew.DAL.GlobalVariables.GetRandomNumber + "")*@

@System.Web.Optimization.Styles.Render("~/bundle/styles/Ecom-table")
@System.Web.Optimization.Scripts.Render("~/bundle/scripts/Ecom-table-Site_Deep_Dive")

<input type="hidden" id="hdn-page" name="hdn-e-commerce-tbl-SiteDeepDive" />
@*Tabs Block*@
<div class="advance-filters" style="height: 47px;background-color: transparent;display:none;">
    @*PIT Tabs*@
    <div id="pit-tabs-block" class="adv-fltr-top" style="height: 68%;display:block;">
        <div class="adv-fltr demo">
            <div class="adv-fltr-option">
                <div class="adv-fltr-label width-1" data-val="DEMOGRAPHIC PROFILING">
                    <span tabindex="1" visit-id-mapping="true" guest-id-mapping="true" visit-spname="usp_EcomWithinSiteTrip" guest-spname="usp_EcomWithinSiteShopper" visit-samplesize-spname="sp_FactBookTripSampleSizeWithin" guest-samplesize-spname="sp_FactBookRespSampleSizeWithin">DEMOGRAPHICS</span>
                </div>
            </div>
        </div>
        <div id="adv-fltr visitsId" class="adv-fltr visits">
            <div class ="filtercenterallign">
            <div class="adv-fltr-label width-5" data-val="PRE-VISITS">
                <span tabindex="2" id-mapping="true" spname="usp_EcomWithinSiteTrip" samplesize-spname="sp_FactBookTripSampleSizeWithin">ONLINE ORDERS</span>
            </div>
            <div class="adv-fltr-label width-5" data-val="IN-VISITS">
                <span tabindex="3" id-mapping="true" spname="usp_EcomWithinSiteTrip" samplesize-spname="sp_FactBookTripSampleSizeWithin">AUTO REPLENISHED DELIVERIES</span>
            </div>
            <div class="adv-fltr-label width-5" data-val="POST-VISITS">
                <span tabindex="4" id-mapping="true" spname="usp_EcomWithinSiteTrip" samplesize-spname="sp_FactBookTripSampleSizeWithin">ITEMS PURCHASED</span>
            </div>
            </div>
        </div>
        <div class="adv-fltr guests">
            <div class="adv-fltr-option guesttabs">
                <div class="adv-fltr-label width-3" data-val="ESTABLISHMENT IMAGERY">
                    <span tabindex="2" id-mapping="true" spname="usp_EcomWithinSiteShopper" samplesize-spname="sp_FactBookRespSampleSizeWithin">SHOPPING FREQUENCY</span>
                </div>
                <div class="adv-fltr-label width-3" data-val="BEVERAGE DETAILS">
                    <span tabindex="3" id-mapping="true" spname="usp_EcomWithinSiteShopper" samplesize-spname="sp_FactBookRespSampleSizeWithin">BEVERAGE PURCHASER</span>
                </div>
            </div>
        </div>
    </div>
    @*End*@
    @*Trend Tabs*@
    <div id="trend-tabs-block" class="adv-fltr-top" style="height: 68%;display:none;">
        <div class="adv-fltr demo">
            <div class="adv-fltr-option">
                <div class="adv-fltr-label width-1" data-val="DEMOGRAPHIC PROFILING">
                    <span tabindex="1" visit-id-mapping="true" guest-id-mapping="true" visit-spname="usp_EcomTrendSiteTrip" guest-spname="usp_EcomTrendSiteShopper" visit-samplesize-spname="sp_FactBookTripOverTimePeriodSampleSize" guest-samplesize-spname="sp_FactBookRespGenOverTimePeriodSampleSize">DEMOGRAPHICS</span>
                </div>
            </div>
        </div>
        <div id="adv-fltr visitsTrendId" class="adv-fltr visits">
            <div class ="filtercenterallign">
            <div class="adv-fltr-label width-5" data-val="PRE-VISITS">
                <span tabindex="2" id-mapping="true" spname="usp_EcomTrendSiteTrip" samplesize-spname="sp_FactBookTripOverTimePeriodSampleSize">ONLINE ORDERS</span>
            </div>
            <div class="adv-fltr-label width-5" data-val="IN-VISITS">
                <span tabindex="3" id-mapping="true" spname="usp_EcomTrendSiteTrip" samplesize-spname="sp_FactBookTripOverTimePeriodSampleSize">AUTO REPLENISHED DELIVERIES</span>
            </div>
            <div class="adv-fltr-label width-5" data-val="POST-VISITS">
                <span tabindex="4" id-mapping="true" spname="usp_EcomTrendSiteTrip" samplesize-spname="sp_FactBookTripOverTimePeriodSampleSize">ITEMS PURCHASED</span>
            </div>
            </div>
        </div>
        <div class="adv-fltr guests">
            <div class="adv-fltr-option guesttabs">
                <div class="adv-fltr-label width-3" data-val="ESTABLISHMENT IMAGERY">
                    <span tabindex="2" id-mapping="true" spname="usp_EcomTrendSiteShopper" samplesize-spname="sp_FactBookRespGenOverTimePeriodSampleSize">SHOPPING FREQUENCY</span>
                </div>
                <div class="adv-fltr-label width-3" data-val="BEVERAGE DETAILS">
                    <span tabindex="3" id-mapping="true" spname="usp_EcomTrendSiteShopper" samplesize-spname="sp_FactBookRespGenOverTimePeriodSampleSize">BEVERAGE PURCHASER</span>
                </div>
            </div>
        </div>
    </div>
    @*End*@
</div>
@*End*@
@*Right Filter Block*@
@Html.Partial("_RightPanelFilter")
@*End*@
<div id="guestFrqncy" style="display:none;" class="tbl-dta-rows"><div class="tbl-dta-freq"><div class="tbl-data-freqbdr"></div><div class="tbl-data-freqtxt">VISITS FREQUENCY</div></div></div>
<div class="tbl-emptyrow"></div>
<div id="Table-Content"></div>
@{
    ViewBag.Title = "iSHOP :: Tables";
}
@*@Styles.Render("~/Content/Tables/Tables.css?" + @iSHOPNew.DAL.GlobalVariables.GetRandomNumber + "")
@Scripts.Render("~/Scripts/Tables/CompareRetailers.js?" + @iSHOPNew.DAL.GlobalVariables.GetRandomNumber + "")
@Scripts.Render("~/Scripts/Tables/Table-Common.js?" + @iSHOPNew.DAL.GlobalVariables.GetRandomNumber + "")*@

@System.Web.Optimization.Styles.Render("~/bundle/styles/table")
@System.Web.Optimization.Scripts.Render("~/bundle/scripts/table-compare_retailer")

<input type="hidden" id="hdn-page" name="hdn-tbl-CompareRetailers" />
@*Tabs Block*@
<div class="advance-filters" style="height: 47px;background-color: transparent;display:none;">
    <div class="adv-fltr-top" style="height: 68%;">
        <div class="adv-fltr demo">
            <div class="adv-fltr-option">
                <div class="adv-fltr-label width-1" data-val="DEMOGRAPHIC PROFILING">
                    <span tabindex="1" visit-id-mapping="true" guest-id-mapping="true" visit-spname="usp_AcrossRetailerTrip" guest-spname="usp_AcrossRetailerShopper" visit-samplesize-spname="sp_FactBookTripSampleSizeAcross" guest-samplesize-spname="sp_FactBookRespondentSampleSizeAcross">DEMOGRAPHICS</span>
                </div>
            </div>
        </div>
        <div id="adv-fltr visitsId" class="adv-fltr visits">
            <div class ="filtercenterallign">
            <div class="adv-fltr-label width-5" data-val="PRE-VISITS">
                <span tabindex="2" id-mapping="true" spname="usp_AcrossRetailerTrip" samplesize-spname="sp_FactBookTripSampleSizeAcross">PRE SHOP</span>
            </div>
            <div class="adv-fltr-label width-5" data-val="IN-VISITS">
                <span tabindex="3" id-mapping="true" spname="usp_AcrossRetailerTrip" samplesize-spname="sp_FactBookTripSampleSizeAcross">IN STORE</span>
            </div>
            <div class="adv-fltr-label width-5" data-val="BEVERAGE DETAILS1">
                <span tabindex="5" id-mapping="true" spname="usp_AcrossRetailerTrip" samplesize-spname="sp_FactBookTripSampleSizeAcross">IN STORE - BEVERAGE DETAIL</span>
            </div>
            <div class="adv-fltr-label width-5" data-val="POST-VISITS">
                <span tabindex="4" id-mapping="true" spname="usp_AcrossRetailerTrip" samplesize-spname="sp_FactBookTripSampleSizeAcross">POST SHOP/TRIP SUMMARY</span>
            </div>
               
            </div>
        </div>
        <div class="adv-fltr guests">
            <div class="adv-fltr-option guesttabs">  
                <div class="adv-fltr-label width-3" data-val="ONLINE METRICS">
                    <span tabindex="4" id-mapping="true" spname="usp_AcrossRetailerShopper" samplesize-spname="sp_FactBookTripSampleSizeAcross">ONLINE METRICS</span>
                </div>             
                <div class="adv-fltr-label width-3" data-val="ESTABLISHMENT IMAGERY">
                    <span tabindex="2" id-mapping="true" spname="usp_AcrossRetailerShopper" samplesize-spname="sp_FactBookRespondentSampleSizeAcross">STORE FREQUENCY/IMAGERY</span>
                </div>
                <div class="adv-fltr-label width-3" data-val="BEVERAGE DETAILS">
                    <span tabindex="3" id-mapping="true" spname="usp_AcrossRetailerShopper" samplesize-spname="sp_FactBookRespondentSampleSizeAcross">BEVERAGE PURCHASER</span>
                </div>
                
            </div>
        </div>
    </div>
    
</div>
@*End*@
@*Right Filter Block*@
@Html.Partial("_RightPanelFilter")
@*End*@

<div id="guestFrqncy" style="display:none;" class="tbl-dta-rows"><div class="tbl-dta-freq"><div class="tbl-data-freqbdr"></div><div class="tbl-data-freqtxt">VISITS FREQUENCY</div></div></div>
<div class="tbl-emptyrow"></div>
<div id="Table-Content"></div>
